# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=chatgpt_panel
DB_SSLMODE=disable

# 服务配置
SERVER_PORT=8080
GIN_MODE=debug

# JWT密钥
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# 浏览器插件对接（可选）
# 留空则不校验扩展请求的 Token
# EXTENSION_TOKEN=your-extension-token

# CLIProxyAPI integration (optional)
# CLIPROXY_BASE_URL=https://augment.zeabur.app
# CLIPROXY_MANAGEMENT_KEY=your-management-key
# CLIPROXY_AUTH_FILE_PREFIX=codex
# CLIPROXY_TIMEOUT_SECONDS=15

# Linux DO OAuth 配置
# 在 https://connect.linux.do/my/applications 创建应用获取
LINUXDO_CLIENT_ID=
LINUXDO_CLIENT_SECRET=

# Linux DO Credit 支付配置
# 联系 Linux DO 获取商户号和密钥
LINUXDO_CREDIT_PID=
LINUXDO_CREDIT_KEY=
LINUXDO_CREDIT_BASE_URL=https://credit.linux.do/epay

